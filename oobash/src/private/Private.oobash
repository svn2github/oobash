__gnustatCheck() {
   __decoratorCheck  "${FUNCNAME[0]}" "${BASH_SOURCE[0]}" "$@"
   # For this framework it is important to work with a gnu stat, so:
   # special check for gnu stat with "--file-system" gnu feature:
   if command -v stat >/dev/null 2>&1; then
