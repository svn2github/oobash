# called by:
# Script.generator
# Class.generator
__localDateFormat() {
   __decoratorCheck  "${FUNCNAME[0]}" "${BASH_SOURCE[0]}" "$@"
   local format
   case $LANG in
      en_US*|en_BZ*)
         format="+%m/%d/%Y"
         ;;
