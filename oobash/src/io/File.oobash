File.createTemporaryDir() {
   __decoratorCheck  "${FUNCNAME[0]}" "${BASH_SOURCE[0]}" "$@"
   declare -i returnValue=0
   if (( $# == 2 )) || (( $# == 3 )); then
      if command -v mktemp >/dev/null 2>&1; then
         if command -v chmod >/dev/null 2>&1; then
            local tempdir=${TMPDIR:-/tmp};
            local prefix="$1"
