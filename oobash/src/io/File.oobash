      if command -v rm >/dev/null 2>&1; then
         if command -v rmdir >/dev/null 2>&1; then
            local this="$1"
            local file="$("$this".toString)"
            if [[ "$("$this".exists)" == "$(Boolean.TRUE)" ]]; then
               rm -f -- "${file}" 2>/dev/null || rmdir -- "${file}" 2>/dev/null
               if (( $? == 0 )); then
                  Boolean.TRUE
               else
                  Boolean.FALSE
               fi
            else
               Boolean.FALSE
